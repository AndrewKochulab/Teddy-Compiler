
enum Expression {
    case Addition(lhs: Int, rhs: Int);
    case Subtraction(minuend: Int, subtrahend: Int);
    case Multiplication(multiplicant: Int, multiplier: Int);
    case Division(dividend: Int, divisor: Int);
}

func evaluate(expression: Expression) -> Expression {
    return expression;
}

