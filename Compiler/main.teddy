
enum ArithmeticType {
    case Add        (lhs: Int, rhs: Int);
    case Subtract   (lhs: Int, rhs: Int);
    case Multiply   (lhs: Int, rhs: Int);
    case Divide     (lhs: Int, rhs: Int);
}

func squared(a: Int) -> Int {
    return a * a;
}

func main() -> Int {
    
    let message: String = "Hello, world!";
    print(message);
    
    let i: Int = 100 + 50;
    
    if (true) {
        print(squared(i));
    }
    
    return 0;
}
